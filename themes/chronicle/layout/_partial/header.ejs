<header class="header" id="header">
  <div class="container header__inner">
    <a class="header__logo" href="<%- url_for('/') %>">
      <%= config.title %>
    </a>
    <button class="header__toggle" id="nav-toggle" aria-label="Toggle navigation">
      <span></span><span></span><span></span>
    </button>
    <nav class="header__nav" id="nav-menu">
      <% for (var key in theme.nav) { %>
        <a class="header__link" href="<%- url_for(theme.nav[key]) %>"><%= key %></a>
      <% } %>
      <% if (theme.rss) { %>
        <a class="header__link header__link--rss" href="<%- url_for(theme.rss) %>" aria-label="RSS">RSS</a>
      <% } %>
    </nav>
  </div>
</header>
