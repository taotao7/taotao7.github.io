<head>
  <meta charset="utf-8">
  <meta name="viewport" content="width=device-width, initial-scale=1, maximum-scale=5">
  <title><%= page.title ? page.title + ' â€” ' + config.title : config.title %></title>
  <meta name="description" content="<%= page.description || page.excerpt || config.description %>">
  <% if (page.keywords) { %>
  <meta name="keywords" content="<%= page.keywords %>">
  <% } else if (config.keywords) { %>
  <meta name="keywords" content="<%= config.keywords %>">
  <% } %>
  <link rel="canonical" href="<%= url %>">
  <% if (theme.rss) { %>
  <link rel="alternate" type="application/atom+xml" title="<%= config.title %>" href="<%= url_for(theme.rss) %>">
  <% } %>
  <link rel="preconnect" href="https://fonts.googleapis.com">
  <link rel="preconnect" href="https://fonts.gstatic.com" crossorigin>
  <link href="https://fonts.googleapis.com/css2?family=Audiowide&family=Metamorphous&family=Space+Grotesk:wght@300..700&display=swap" rel="stylesheet">
  <%- css('css/style') %>
</head>
